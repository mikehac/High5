{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "emitDeclarationOnly": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": ["node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2020",
    "sourceMap": true,
    "paths": {
      "@high5/interfaces/*": ["./libs/iterfaces/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "references": []
}
